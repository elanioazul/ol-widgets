{
    "version": 8,
    "name": "VectorTiles GeoServer",
    "id": "vectortiles-geoserver",
    "metadata": {
      "mapbox:autocomposite": false,
      "mapbox:type": "template",
      "maputnik:renderer": "mbgljs",
      "openmaptiles:version": "3.x"
    },
    "sources": {
      "geoformas": {
        "tiles": ["http://localhost:8080/geoserver/gwc/service/tms/1.0.0/geoformas@EPSG%3A900913@pbf/{z}/{x}/{-y}.pbf"],
        "scheme": "tms",
        "type": "vector"
      }
    },
    "layers": [
        {
            "id": "Fondo de valle o vertiente",
            "source": "geoformas",
            "source-layer": "ge.geomorf_cyl_ad_di_s",
            "filter": ["==", "TIPO", "Fondo de valle o vertiente"],
            "type": "fill",
            "paint": {
                "fill-color": "#a6611a",
                "fill-outline-color": "#232323"
            }

        },
        {
            "id": "Masa de agua",
            "source": "geoformas",
            "source-layer": "ge.geomorf_cyl_ad_di_s",
            "filter": ["==", "TIPO", "Masa de agua"],
            "type": "fill",
            "paint": {
                "fill-color": "#c08c46",
                "fill-outline-color": "#232323"
            }

        },
        {
            "id": "Relieve estructural",
            "source": "geoformas",
            "source-layer": "ge.geomorf_cyl_ad_di_s",
            "filter": ["==", "TIPO", "Relieve estructural"],
            "type": "fill",
            "paint": {
                "fill-color": "#d9b772",
                "fill-outline-color": "#232323"
            }

        },
        {
            "id": "Superficie de erosión degradada miocena",
            "source": "geoformas",
            "source-layer": "ge.geomorf_cyl_ad_di_s",
            "filter": ["==", "TIPO", "Superficie de erosión degradada miocena"],
            "type": "fill",
            "paint": {
                "fill-color": "#e7d3a5",
                "fill-outline-color": "#232323"
            }

        },
        {
            "id": "Superficie de erosión degradada pleistoc. inf. med.",
            "source": "geoformas",
            "source-layer": "ge.geomorf_cyl_ad_di_s",
            "filter": ["==", "TIPO", "Superficie de erosión degradada pleistoc. inf. med."],
            "type": "fill",
            "paint": {
                "fill-color": "#f1eadb",
                "fill-outline-color": "#232323"
            }

        },
        {
            "id": "Superficie de erosión degradada pliocena",
            "source": "geoformas",
            "source-layer": "ge.geomorf_cyl_ad_di_s",
            "filter": ["==", "TIPO", "Superficie de erosión degradada pliocena"],
            "type": "fill",
            "paint": {
                "fill-color": "#dbedea",
                "fill-outline-color": "#232323"
            }

        },
        {
            "id": "Superficie de erosión retocada miocena",
            "source": "geoformas",
            "source-layer": "ge.geomorf_cyl_ad_di_s",
            "filter": ["==", "TIPO", "Superficie de erosión retocada miocena"],
            "type": "fill",
            "paint": {
                "fill-color": "#a7dbd3",
                "fill-outline-color": "#232323"
            }

        },
        {
            "id": "Superficie de erosión retocada pleistoc. inf. med.",
            "source": "geoformas",
            "source-layer": "ge.geomorf_cyl_ad_di_s",
            "filter": ["==", "TIPO", "Superficie de erosión retocada pleistoc. inf. med."],
            "type": "fill",
            "paint": {
                "fill-color": "#72c5b8",
                "fill-outline-color": "#232323"
            }

        },
        {
            "id": "Superficie de erosión retocada pliocena",
            "source": "geoformas",
            "source-layer": "ge.geomorf_cyl_ad_di_s",
            "filter": ["==", "TIPO", "Superficie de erosión retocada pliocena"],
            "type": "fill",
            "paint": {
                "fill-color": "#39a595",
                "fill-outline-color": "#232323"
            }

        },
        {
            "id": "ge.geomorf_cyl_ad_di_l-labels",
            "source": "geoformas",
            "source-layer": "ge.geomorf_cyl_ad_di_l",
            "type": "symbol",
            "layout": {
                "text-field": "{TIPO}",
                "symbol-placement": "line",
                "text-offset": [0, -8],
                "text-padding": "10",
                "text-color": "white"
            },
            "paint": {
                "text-color": "black"
            }
        },
        {
            "id": "ge.geomorf_cyl_ad_di_l",
            "source": "geoformas",
            "source-layer": "ge.geomorf_cyl_ad_di_l",
            "type": "line",
            "paint": {
                "line-color": "brown",
                "line-width": 1,
                "line-dasharray": [5, 2]
            }
        }
    ]
  }